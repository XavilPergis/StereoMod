{
  "schemaVersion": 1,
  "id": "testbed",
  "version": "${version}",
  "name": "Testbed",
  "description": "A testbed for lots of little modding ideas!",
  "authors": [
    "Avi"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
